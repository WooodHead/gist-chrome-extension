{
  "name": "Github Gist Extension",
  "short_name": "Gist Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "easily connect the gist",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
  {
    "matches": [
      "*://github.com/*"
    ],
    "js": ["src/jquery.js","src/background.js"]
  }
]
 }
